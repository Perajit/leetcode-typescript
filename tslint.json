{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": ["node_modules/**/*"]
    },
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "semicolon": [true, "always"],
        "no-console": false,
        "no-empty": false,
        "prefer-for-of": false,
        "forin": false,
        "max-classes-per-file": false,
        "jsdoc-format": false
    },
    "rulesDirectory": []
}